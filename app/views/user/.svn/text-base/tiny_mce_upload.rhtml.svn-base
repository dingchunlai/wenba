<div id="upclear" style="width:98%;" align=center>
  <form name="form1" id="form1" method="post" action="save_upload" enctype="multipart/form-data" target='uploadwindow'>
    <fieldset>
      <legend>图片来源</legend>
      <table border=0 cellpadding=0 cellspacing=0>
        <tr><td colspan=9 height=5></td></tr>
        <tr>
          <%#*<td width=1></td>%>
          <td width=60 align=right onclick="RadioClick('file')"><input type=radio id="d_checkfromfile" value="1" onclick="RadioClick('file')" checked><span id="text1">上传:</span></td>
          <td width=5></td>
          <td colspan=5>
            <input id="ask_user_upload_uploaded_data" name="ask_user_upload[uploaded_data]" size="30" type="file" />
          </td>
          <td width=7></td>
        </tr>
        <%#*<tr><td colspan=9 height=5></td></tr>%>
        <tr>
          <%#*<td width=1></td>%>
          <td width=60 align=right onclick="RadioClick('url')"><input type=radio id="d_checkfromurl" value="1" onclick="RadioClick('url')"><span id="text2" style="color:#666666">网络:</span></td>
          <td width=5></td>
          <td colspan=5><input id=d_fromurl type=text  value="http://" size=42 disabled><div id=loadscript></div></td>
          <td width=7></td>
        </tr>
        <%#*<tr><td colspan=9 height=5></td></tr>%>
        <tr><td colspan=9 height=5 align=center>&nbsp;&nbsp;<input type="submit" name="Submit1" id="Submit1" value=" 确 定 " onclick="return checkData()">&nbsp;&nbsp;<input type=button value=' 取 消 ' onclick="window.close();"></td></tr>
        <tr><td colspan=9 height=5></td></tr>
      </table>
    </fieldset>
  </form>
</div><iframe src="" name=uploadwindow frameborder=0 scrolling=no vspace=0 hspace=0 marginheight=0 marginwidth=0 height=0 width=0></iframe>
<div id=divProcessing style="width:200px;height:30px;position:absolute;left:70px;top:15px;display:none">
  <table border=0 cellpadding=0 cellspacing=1 width="100%" height="100%"><tr><td><img src="/images/spinner.gif"></td></tr></table>
</div>